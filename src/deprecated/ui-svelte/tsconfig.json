{
	"compilerOptions": {
		"target": "ESNext",
		"useDefineForClassFields": true,
		"module": "ESNext",
		"allowJs": true,
		"checkJs": true,
		"isolatedModules": true,
		"lib": [
			"ESNext"
		],
		"moduleResolution": "bundler"
	},
	"include": [
		"src/**/*.d.ts",
		"src/**/*.ts",
		"src/**/*.js",
		"src/**/*.svelte",
		"test/*.js",
		"../../../global.d.ts"
	]
}
