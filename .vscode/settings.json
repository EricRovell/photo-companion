{
	"files.exclude": {
		"**/.git": true,
	},
	"search.exclude": {
		"**/node_modules": true
	},
	"files.associations": {
		"*.mdx": "markdown"
	},
	"eslint.experimental.useFlatConfig": true,
	"eslint.workingDirectories": [
		"./packages/*"
	],
	"css.lint.validProperties": [
		"cx",
		"cy",
		"r"
	],
	"cSpell.language": "en,ru,en-US,ru-RU",
	"cSpell.words": [
		"cведены",
		"fmod",
		"Большеохтинского",
		"Гренадёрского",
		"Темнейший"
	],
	"catppuccin-icons.associations.extensions": {
		"tsx": "solid"
	}
}
